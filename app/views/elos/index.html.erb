<p id="notice"><%= notice %></p>

<h1>Listing Elos</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Value</th>
      <th>Sample date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elos.each do |elo| %>
      <tr>
        <td><%= elo.team %></td>
        <td><%= elo.value %></td>
        <td><%= elo.sample_date %></td>
        <td><%= link_to 'Show', elo %></td>
        <td><%= link_to 'Edit', edit_elo_path(elo) %></td>
        <td><%= link_to 'Destroy', elo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elo', new_elo_path %>
