<div ng-controller="ElosController" ng-init="initialize()">
  <div class="container-fluid">
    <div class="row active progress-bar progress-bar-striped progress-bar-primary"
      ng-show="loading_message"
      role="progressbar"
      aria-valuenow="100"
      aria-valuemin="0"
      aria-valuemax="100"
      style="width: 100%">
      {{loading_message}}
    </div>
  </div>
  <div google-chart chart="eloChart" on-ready="clear_progress()" style="height:600px; width:100%;">
  </div>
</div>
