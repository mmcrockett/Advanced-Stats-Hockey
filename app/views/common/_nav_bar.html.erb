<% links = [:seasons, :teams, :games, :elos, :graph, :lines] %>
<% if (true == logged_in?) %>
  <% links << :logout %>
<% else %>
  <% links << :login %>
<% end %>
      <div class="navbar navbar-default" id="nav-bar">
        <ul class="nav navbar-nav" style="width:100%">
        <% links.each do |link| %>
          <li style="width:<%= (100 / links.size).floor %>%"><a href="/<%= link %>"><%= link %></a></li>
        <% end %>
        </ul>
      </div>
